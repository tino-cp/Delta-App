end

function defineKersDirection(valuePrv, valueCur)
  if valuePrv &gt; valueCur then storeUsedKers(valuePrv - valueCur)
  elseif valuePrv &lt; valueCur then storeGainedKers(valueCur - valuePrv)
  end
end

